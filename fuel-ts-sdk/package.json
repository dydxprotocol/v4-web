{
  "name": "fuel-ts-sdk",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./trading": {
      "types": "./dist/trading/index.d.ts",
      "import": "./dist/trading/index.js"
    },
    "./accounts": {
      "types": "./dist/accounts/index.d.ts",
      "import": "./dist/accounts/index.js"
    },
    "./cross-domain": {
      "types": "./dist/cross-domain/index.d.ts",
      "import": "./dist/cross-domain/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm codegen && rollup -c",
    "dev": "rollup -c --watch",
    "typecheck": "pnpm tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "codegen": "graphql-codegen --config ./codegen.yml",
    "postinstall": "pnpm codegen",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@fuels/connectors": "^0.43.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@starboard/indexer": "workspace:*",
    "@types/lodash": "^4.17.21",
    "fuels": "0.101.3",
    "graphql": "^16.8.1",
    "graphql-request": "^7.3.5",
    "graphql-ws": "^6.0.6",
    "immer": "^11.1.3",
    "lodash": "^4.17.21",
    "reselect": "^5.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.0.3",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "~5.9.3",
    "vitest": "^3.0.9"
  }
}
