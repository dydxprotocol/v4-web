{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "public/*": ["./public/*"],
      "fuel-ts-sdk": ["../fuel-ts-sdk/src/index.ts"],
      "fuel-ts-sdk/client": ["../fuel-ts-sdk/src/client.ts"],
      "fuel-ts-sdk/trading": ["../fuel-ts-sdk/src/Trading/index.ts"],
      "@sdk/*": ["../fuel-ts-sdk/src/*"]
    },
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.base.tsbuildinfo"
  }
}
