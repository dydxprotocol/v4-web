    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "clsx": "^2.1.1",
    "fuel-ts-sdk": "workspace:*",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.11.0",
    "tailwindcss": "^4.1.18",
    "vite-envs": "^4.6.2"
  },
  "devDependencies": {
    "@react-router/dev": "^7.11.0",
