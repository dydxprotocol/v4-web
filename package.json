    "@react-types/table": "^3.6.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@skip-go/client": "1.1.7",
    "@solana/web3.js": "^1.93.0",
    "@statsig/js-client": "1.4.0",
    "@statsig/react-bindings": "1.4.0",
    "@tanstack/react-query": "^5.37.1",
    "@tanstack/react-virtual": "^3.13.5",
    "@visx/axis": "^3.1.0",
    "@visx/curve": "^3.0.0",
    "@visx/event": "^3.0.1",
