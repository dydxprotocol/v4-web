    "@visx/point": "^3.0.1",
    "@visx/responsive": "^3.0.0",
    "@visx/scale": "^3.0.0",
    "@visx/shape": "^3.0.0",
    "@visx/threshold": "^3.0.0",
    "@visx/tooltip": "^3.1.2",
    "@visx/xychart": "^3.1.2",
    "@wagmi/core": "^2.16.3",
    "bignumber.js": "^9.1.1",
    "cmdk": "^0.2.0",
    "crypto-js": "^4.1.1",
    "cuer": "^0.0.2",
    "export-to-csv": "^1.2.3",
    "fast-json-stable-stringify": "^2.1.0",
    "fuel-ts-sdk": "workspace:*",
    "fuels": "^0.101.3",
    "graz": "^0.1.19",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "long": "^5.2.3",
    "luxon": "^3.3.0",
    "mipd": "^0.0.7",
    "react": "^18.2.0",
    "react-aria": "^3.25.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.2.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.14.0",
    "react-stately": "^3.23.0",
    "redux-persist": "^6.0.0",
    "reselect": "^5.1.0",
    "starboard-client-js": "file:./packages/starboard-client-js-1.3.17.tgz",
    "styled-components": "^6.1.12",
    "twin.macro": "^3.4.1",
    "typia": "^7.1.0",
    "unionize": "^3.1.0",
    "use-latest": "^1.2.1",
    "viem": "^2.22.17",
    "wagmi": "^2.14.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^16.5.0",
    "pnpm": "^8.6.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^3.0.9"
  },
  "pnpm": {
    "overrides": {
      "typescript": "~5.9.3",
      "graphql": "^15.10.1",
      "vitest": "^3.0.9"
    }
  }
}
