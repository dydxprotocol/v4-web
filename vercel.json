{
  "headers": [
    {
      "source": "/entry-points/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/currencies/(.*\\.png)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, immutable" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, immutable" }
      ]
    },
    {
      "source": "/tradingview/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, s-maxage=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|tradingview/|currencies/|favicon.ico|robots.txt|sitemap.xml).*)",
      "destination": "/entry-points/index.html"
    }
  ]
}
