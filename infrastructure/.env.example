# Price Feed Service - Deployment Configuration
# Copy this file to .env and fill in your values

# Required: Fuel Network RPC endpoint
# Example: https://testnet.fuel.network/v1/graphql
FUEL_RPC_URL=

# Required: Deployed StorkMock contract address on Fuel blockchain
# Example: 0x1234567890abcdef...
CONTRACT_ADDRESS=

# Required: Private key for wallet authorized to update prices
# This will be stored encrypted in AWS Secrets Manager
PRICE_SIGNER_PRIVATE_KEY=

# Optional: EventBridge schedule expression (default: rate(5 minutes))
# Examples: rate(5 minutes), rate(1 hour), cron(0/5 * * * ? *)
SCHEDULE_EXPRESSION=rate(5 minutes)

# Optional: ECS task CPU in units (default: 256 = 0.25 vCPU)
# Options: 256, 512, 1024, 2048
TASK_CPU=256

# Optional: ECS task memory in MB (default: 512)
# Options: 512, 1024, 2048, 4096
TASK_MEMORY=512

# Optional: VPC CIDR block (default: 10.0.0.0/16)
VPC_CIDR=10.0.0.0/16

# Optional: AWS region (default: us-east-1)
AWS_REGION=us-east-1

# Optional: CloudFormation stack name (default: price-feed-service-stack)
STACK_NAME=price-feed-service-stack
