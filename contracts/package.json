{
    "name": "starboard/contracts",
    "version": "1.0.0",
    "type": "module",
    "main": "node.js",
    "license": "Apache-2.0",
    "engines": {
        "node": ">=22.0.0"
    },
    "scripts": {
        "clean": "forc clean",
        "build:vault-expose-1": "node scripts/copy-vault-src.mjs",
        "build:vault-expose-2": "node scripts/concat-vault-expose.mjs",
        "build:contracts": "forc build --release --log-level 4",
        "build:pre": "pnpm build:vault-expose-1 && pnpm build:vault-expose-2",
        "build": "pnpm build:pre && pnpm build:contracts",
        "gen:types": "fuels typegen -i ./contracts/**/*-abi.json -o ./types && node scripts/fix-typegen-imports.mjs",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint:contracts": "forc fmt --check",
        "lint": "forc fmt --check && eslint test/ tasks/",
        "lint:fix": "eslint --fix test/ tasks/",
        "run:node": "tsx tasks/run-node.ts",
        "deploy:testnet:token": "tsx tasks/deploy-testnet-token.ts",
        "deploy:starboard": "tsx tasks/deploy-starboard.ts",
        "deploy:stork-mock": "tsx tasks/deploy-stork-mock.ts",
        "setup:testnet": "tsx tasks/setup-testnet.ts",
        "prices:feed": "tsx tasks/prices-feed.ts",
        "faucet": "tsx tasks/faucet.ts"
    },
    "devDependencies": {
        "@types/node": "20.3.1",
        "dotenv": "^8.2.0",
        "husky": "8.0.3",
        "prettier": "3.1.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "~5.9.3",
        "vitest": "^3.0.9"
    },
    "dependencies": {
        "bn.js": "^5.2.0",
        "concat": "^1.0.3",
        "fuels": "0.101.3"
    }
}
