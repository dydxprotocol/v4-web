        "chai": "^4.3.0",
        "chai-bn": "^0.2.1",
        "dotenv": "^8.2.0",
        "ethers": "^5.0.31",
        "hardhat": "2.22.5",
        "husky": "8.0.3",
        "patch-package": "8.0.0",
        "prettier": "3.1.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.5"
