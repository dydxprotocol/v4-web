{
  "name": "@starboard/indexer",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "gen:model": "squid-typeorm-codegen",
    "gen:migration": "squid-typeorm-migration generate",
    "apply:migration": "squid-typeorm-migration apply",
    "start": "ts-node -r dotenv/config src/main.ts",
    "start:e2e": "ts-node -r dotenv/config src/main.ts dotenv_config_path=.env.indexer-e2e",
    "test:e2e": "vitest --mode indexer-e2e run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-aggregates": "^0.1.1",
    "@graphile/pg-pubsub": "^4.14.1",
    "@subsquid/batch-processor": "^0.0.0",
    "@subsquid/cli": "^3.2.3",
    "@subsquid/fuel-objects": "^0.0.4",
    "@subsquid/fuel-stream": "^1.0.2",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@types/pg": "^8.15.6",
    "docker-compose": "^1.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "fuels": "0.101.3",
    "graphile-build": "^4.14.1",
    "pg": "^8.13.0",
    "postgraphile": "^4.14.1",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "typeorm": "^0.3.20",
    "vitest": "1.4.0"
  },
  "devDependencies": {
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vitest": "^3.0.9"
  }
}
